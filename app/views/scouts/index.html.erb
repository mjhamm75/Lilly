<table class="table table-striped">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Birthday</th>
    </tr>
  </thead>
  <tbody>
    <% @scouts.each do |scout| %>
    <tr>
      <td><%= scout.first_name %></td>
      <td><%= scout.last_name %></td>
      <td><%= scout.birthdate %></td>
    </tr>
    <% end %>
  </tbody>
</table>